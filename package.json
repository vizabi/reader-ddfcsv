{
  "name": "vizabi-ddfcsv-reader",
  "version": "0.6.1",
  "description": "Vizabi DDFcsv reader",
  "main": "dist/bundle.js",
  "scripts": {
    "test": "mocha --compilers ts:ts-node/register --timeout 200000 test/**/*.spec.ts",
    "n-test": "mocha --compilers ts:ts-node/register --timeout 200000 test/**/*.spec.ts",
    "test-travis": "tsc && istanbul cover mocha _mocha -- -R spec --compilers ts:ts-node/register --recursive --timeout 200000 test/**/*.spec.ts && codecov",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "github-release": "conventional-github-releaser -p angular",
    "build": "tsc && webpack && WEB_ENV=1 webpack",
    "preversion": "npm test",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push origin master && git push --tags && npm run github-release",
    "prepublish": "npm run build && PROD_ENV=1 webpack"
  },
  "author": "Vyacheslav Chub<vyacheslav.chub@valor-software.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "@types/async": "2.0.34",
    "@types/bluebird": "3.0.36",
    "@types/csv-parse": "1.1.6",
    "@types/lodash": "4.14.43",
    "@types/traverse": "0.6.29",
    "async-es": "2.1.4",
    "bluebird": "3.4.6",
    "csv-parse": "1.1.7",
    "ddf-time-utils": "0.1.6",
    "fetch-polyfill": "0.8.2",
    "lodash-es": "4.17.2",
    "mingo": "0.8.0",
    "papaparse": "^4.1.2",
    "traverse": "0.6.6"
  },
  "devDependencies": {
    "@types/chai": "3.4.34",
    "@types/mocha": "2.2.33",
    "@types/node": "0.0.2",
    "babel": "6.5.2",
    "babel-core": "6.18.2",
    "babel-loader": "6.2.8",
    "babel-preset-es2015": "6.18.0",
    "chai": "3.5.0",
    "mocha": "3.2.0",
    "ts-node": "1.7.0",
    "typescript": "2.1.4",
    "webpack": "1.14.0",
    "codecov": "1.0.1",
    "compare-version": "0.1.2",
    "conventional-changelog": "1.1.0",
    "conventional-changelog-cli": "1.2.0",
    "conventional-github-releaser": "1.1.3",
    "istanbul": "0.4.5"
  }
}

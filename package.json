{
  "name": "vizabi-ddfcsv-reader",
  "version": "2.2.1",
  "description": "Vizabi DDFcsv reader",
  "main": "dist/vizabi-ddfcsv-reader-node.js",
  "scripts": {
    "tslint": "./node_modules/.bin/tslint -c ./tslint.json 'src/**/*.ts' && ./node_modules/.bin/tslint -c ./test/tslint.json 'test/**/*.ts'",
    "tsc": "./node_modules/.bin/tsc --project tsconfig.json && npm run tslint",
    "preinstall": "git submodule update --remote --init -f && rm -rf coverage && rm -rf .nyc_output && rm -rf coverage.lcov",
    "postinstall": "npm run tsc",
    "pree2e": "npm i",
    "e2e": "nyc mocha",
    "e2e:doc": "mocha --require ts-node/register -R markdown > API.SPECIFICATION.md --recursive test/*.spec.ts",
    "test": "npm run e2e",
    "test-travis": "npm run e2e && nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "github-release": "conventional-github-releaser -p angular",
    "build": "npm run tsc && webpack && WEB_ENV=1 webpack",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push origin master && git push --tags && npm run github-release",
    "prepublish": "npm run build && PROD_ENV=1 webpack",
    "deploy": "node deploy.js"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "author": "Vyacheslav Chub<vyacheslav.chub@valor-software.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "fetch-polyfill": "0.8.2",
    "lodash": "4.17.4",
    "papaparse": "4.3.6"
  },
  "devDependencies": {
    "@types/chai": "4.0.10",
    "@types/core-js": "0.9.43",
    "@types/lodash": "4.14.91",
    "@types/mocha": "5.2.1",
    "@types/node": "8.5.2",
    "babel": "6.23.0",
    "babel-core": "6.24.1",
    "babel-loader": "6.4.1",
    "babel-preset-es2015": "6.24.1",
    "chai": "4.1.2",
    "codecov": "3.0.2",
    "compare-version": "0.1.2",
    "conventional-changelog": "1.1.3",
    "conventional-changelog-cli": "1.3.1",
    "conventional-github-releaser": "1.1.3",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "rimraf": "2.6.1",
    "shelljs": "0.7.8",
    "sinon": "4.4.10",
    "source-map-support": "0.5.6",
    "ts-node": "6.1.0",
    "tslint": "5.9.1",
    "typescript": "2.9.1",
    "webpack": "1.14.0"
  }
}

{
  "name": "vizabi-ddfcsv-reader",
  "version": "2.3.0",
  "description": "Vizabi DDFcsv reader",
  "author": "Vyacheslav Chub<vyacheslav.chub@valor-software.com>",
  "license": "GPL-3.0",
  "main": "lib/index.js",
  "scripts": {
    "webpack": "webpack",
    "tslint:src": "./node_modules/.bin/tslint -c ./tslint.json 'src/**/*.ts'",
    "tslint:tests": "./node_modules/.bin/tslint -c ./test/tslint.json 'test/**/*.ts'",
    "tslint": "npm run tslint:src && npm run tslint:tests",
    "tsc": "tsc --project tsconfig.json",
    "submodules:init": "git submodule update --remote --init -f",
    "coverage:clean": "rm -rf coverage && rm -rf .nyc_output && rm -rf coverage.lcov",
    "pree2e": "npm run submodules:init && npm run coverage:clean && npm run tslint && npm run tsc",
    "e2e": "nyc mocha",
    "pree2e:doc": "npm run pree2e",
    "e2e:doc": "mocha --require ts-node/register -R markdown > API.SPECIFICATION.md --recursive test/*.spec.ts",
    "test": "npm run e2e",
    "test-travis": "npm run e2e && nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "github-release": "conventional-github-releaser -p angular",
    "build": "npm run build:node && npm run build:web",
    "build:node": "WEB_ENV=0 npm run webpack -- --env.mode production",
    "build:web": "WEB_ENV=1 npm run webpack -- --env.mode production",
    "build:dev": "npm run build:node:dev && npm run build:web:dev",
    "build:node:dev": "WEB_ENV=0 npm run webpack -- --env.mode development",
    "build:web:dev": "WEB_ENV=0 npm run webpack -- --env.mode development",
    "preversion": "npm run build",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push origin master && git push --tags && npm run github-release",
    "prepublish": "npm run build",
    "deploy": "node deploy.js"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "@types/core-js": "2.5.0",
    "@types/node": "10.5.7",
    "ddf-query-validator": "1.0.2",
    "fetch-polyfill": "0.8.2",
    "lodash.clonedeep": "4.5.0",
    "lodash.compact": "3.0.1",
    "lodash.every": "4.6.0",
    "lodash.filter": "4.6.0",
    "lodash.first": "3.0.0",
    "lodash.flatmap": "4.5.0",
    "lodash.flattendeep": "4.4.0",
    "lodash.has": "4.5.2",
    "lodash.head": "4.0.1",
    "lodash.includes": "4.3.0",
    "lodash.isarray": "4.0.0",
    "lodash.isempty": "4.4.0",
    "lodash.isnil": "4.0.0",
    "lodash.isobject": "3.0.2",
    "lodash.isstring": "4.0.1",
    "lodash.keys": "4.2.0",
    "lodash.map": "4.6.0",
    "lodash.set": "4.3.2",
    "lodash.size": "4.2.0",
    "lodash.startswith": "4.2.1",
    "lodash.trimstart": "4.5.1",
    "lodash.uniq": "4.5.0",
    "lodash.values": "4.3.0",
    "papaparse": "4.3.6"
  },
  "devDependencies": {
    "@types/chai": "4.1.4",
    "@types/lodash": "4.14.116",
    "@types/mocha": "5.2.5",
    "chai": "4.1.2",
    "clean-webpack-plugin": "0.1.19",
    "codecov": "3.0.4",
    "compare-version": "0.1.2",
    "compression-webpack-plugin": "1.1.11",
    "conventional-changelog": "1.1.3",
    "conventional-changelog-cli": "1.3.1",
    "conventional-github-releaser": "1.1.9",
    "copy-webpack-plugin": "4.5.2",
    "html-webpack-plugin": "3.2.0",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "rimraf": "2.6.2",
    "shelljs": "0.8.2",
    "sinon": "6.1.4",
    "source-map-loader": "0.2.3",
    "source-map-support": "0.5.6",
    "ts-babel-node": "1.1.1",
    "ts-loader": "4.3.0",
    "ts-node": "7.0.0",
    "tslib": "1.9.3",
    "tslint": "5.11.0",
    "typescript": "3.0.1",
    "uglifyjs-webpack-plugin": "1.2.5",
    "webpack": "4.8.1",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "3.1.4",
    "webpack-merge": "4.1.2",
    "webpack-node-externals": "1.7.2"
  }
}
